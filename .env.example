# ============================================
# REQUIRED - Core Configuration
# ============================================

# Database
POSTGRES_USER=rootUser
POSTGRES_PWD=mypassword

# Security (CHANGE IN PRODUCTION!)
JWT_SECRET_KEY=your_jwt_secret_change_this_in_production

# Application URLs - Network Access
PUBLIC_FRONT_URL=http://localhost:3000,http://192.168.1.75:3000
PUBLIC_API_URL=http://192.168.1.75:8080

# MinIO Storage
STORAGE_TYPE=minio
MINIO_USER=minio
MINIO_PASSWORD=minio123
PUBLIC_MINIO_ENDPOINT=http://192.168.1.75:9000

# ============================================
# OPTIONAL - Leave empty/commented if not using
# ============================================

# Email/SMTP
#ENABLE_EMAIL_NOTIFICATIONS=true
#INVITATION_VIA_EMAIL=true
#SMTP_HOST=smtp.gmail.com
#SMTP_PORT=587
#SMTP_USER=your-email@gmail.com
#SMTP_PWD=your-password
#MAIL_RECIPIENTS=admin@example.com

# Google Services
#GOOGLE_KEY=your-google-maps-api-key
#GOOGLE_TRACKING_ID=UA-XXXXXXXXX-X

# MUI X License
#MUI_X_LICENSE=your-license-key

# SSO/OAuth2
#ENABLE_SSO=true
#OAUTH2_PROVIDER=GOOGLE
#OAUTH2_CLIENT_ID=your-client-id
#OAUTH2_CLIENT_SECRET=your-client-secret

# Google Cloud Storage (alternative to MinIO)
#GCP_BUCKET_NAME=your-bucket
#GCP_JSON={"your":"credentials"}
#GCP_PROJECT_ID=your-project

# White Labeling
#LOGO_PATHS={"white":"/logo-white.png","dark":"/logo-dark.png"}
#CUSTOM_COLORS={"primary":"#1976d2","secondary":"#dc004e"}
#BRAND_CONFIG={"name":"YourCompany","website":"https://example.com"}

# Licensing
#LICENSE_KEY=your-license-key
#ALLOWED_ORGANIZATION_ADMINS=admin1@example.com,admin2@example.com

# Other
#DEMO_LINK=https://demo.example.com
#CLOUD_VERSION=true