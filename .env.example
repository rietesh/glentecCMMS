# Email Configuration
ENABLE_EMAIL_NOTIFICATIONS=false
INVITATION_VIA_EMAIL=false
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PWD=
MAIL_RECIPIENTS=

# Database
POSTGRES_USER=rootUser
POSTGRES_PWD=mypassword

# Security
JWT_SECRET_KEY=your_jwt_secret_change_this_in_production

# Application URLs - Network Access Configuration
# Frontend accepts requests from both localhost and network IP
PUBLIC_FRONT_URL=http://localhost:3000,http://192.168.1.75:3000
# API URL for frontend to connect to backend
PUBLIC_API_URL=http://192.168.1.75:8080

# Storage (MinIO) - IMPORTANT: Two different endpoints!
STORAGE_TYPE=minio
MINIO_USER=minio
MINIO_PASSWORD=minio123
# Internal endpoint - backend uses this (Docker service name)
MINIO_ENDPOINT=http://minio:9000
# Public endpoint - frontend/browser uses this (your machine's IP)
PUBLIC_MINIO_ENDPOINT=http://192.168.1.75:9000

# Google Cloud Storage (alternative to MinIO)
GCP_BUCKET_NAME=
GCP_JSON=
GCP_PROJECT_ID=

# Google Services
GOOGLE_KEY=
GOOGLE_TRACKING_ID=

# MUI License
MUI_X_LICENSE=

# SSO/OAuth
ENABLE_SSO=false
OAUTH2_PROVIDER=
OAUTH2_CLIENT_ID=
OAUTH2_CLIENT_SECRET=

# Licensing & Branding
LICENSE_KEY=
LICENSE_FINGERPRINT_REQUIRED=true
ALLOWED_ORGANIZATION_ADMINS=
LOGO_PATHS=
CUSTOM_COLORS=
BRAND_CONFIG=
DEMO_LINK=

# Deployment
CLOUD_VERSION=false